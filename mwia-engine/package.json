    "@types/jest": "^29.4.0",
    "@types/node-fetch": "^2.6.4",
    "jest": "^29.5.0",
    "jest-transform-stub": "^2.0.0",
    "rollup": "^2.74.0",
    "rollup-plugin-copy": "^3.4.0",
    "ts-jest": "^29.0.5",
